namespace A {
    class B {
        public static function call() {
            return 'Foo Bar';
        }
    }

    function C() {
        return 'Bar Foo';
    }

    const ABC = 'DEF';
    define('GHI', 'JKL');

    echo __NAMESPACE__, "\n";

    echo B::call(), "\n";
    echo C(), "\n";
    echo ABC, "\n";
    echo GHI, "\n";
}
